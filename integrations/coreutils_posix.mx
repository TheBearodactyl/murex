autocomplete set man %[{
    IncExePath: true
}]

autocomplete set man-summary %[{
    IncExePath:    true
    AllowMultiple: true
}]

autocomplete set sudo %[
    {
        IncFiles:   true
        IncDirs:    true
        IncExePath: true
    }
    {
        NestedCommand: true
    }
]

#alias ls=ls --color=auto
alias grep=grep --color=auto
alias egrep=grep -E --color=auto

$ENV.CLICOLOR = true